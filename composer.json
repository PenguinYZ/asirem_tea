{
    "name": "penguinyz/asirem_tea",
    "description": "The weird Flarum theme made with potatoes as fuel, by Afrux. Modified for Teaet",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.8.0",
        "afrux/flarum-theme-base": "^0.1.0"
    },
    "require-dev": {
        "sycho/flarum-uikit": "0.1.x-dev",
        "afrux/flarum-theme-base": "0.1.x-dev"
    },
    "authors": [
        {
            "name": "PenguinYZ",
            "email": "penguinyz@outlook.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/PenguinYZ/asirem_tea/issues",
        "source": "https://github.com/PenguinYZ/asirem_tea"
    },
    "autoload": {
        "psr-4": {
            "PenguinYZ\\Asirem_Tea\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PenguinYZ\\Asirem_Tea\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Asirem_Tea",
            "category": "theme",
            "optional-dependencies": [
                "flarum/tags",
                "flarum/approval"
            ],
            "icon": {
                "image": "icon.svg"
            }
        },
        "flarum-cli": {
            "modules": {
                "admin": true,
                "forum": true,
                "js": true,
                "jsCommon": false,
                "css": true,
                "gitConf": true,
                "githubActions": true,
                "prettier": true,
                "typescript": true,
                "bundlewatch": false,
                "backendTesting": false,
                "editorConfig": true,
                "styleci": true
            }
        }
    }
}
